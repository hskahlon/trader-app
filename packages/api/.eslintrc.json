{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["standard"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "semi": "off",
    "react/prop-types": "off",
    "quotes": "off",
    "comma-dangle": "off",
    "no-undef": "off",
    "space-before-function-paren": "off",
    "no-unused-vars": "off"
  }
}
